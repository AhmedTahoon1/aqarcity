{
  "name": "aqar-city-uae",
  "version": "1.0.0",
  "description": "منصة عقارية ديناميكية ذكية 100% موجهة لسوق الإمارات العربية المتحدة",
  "type": "module",
  "scripts": {
    "dev:server": "tsx watch server/index.ts",
    "dev:client": "cd client && npm run dev",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cd client && npm run build",
    "build:server": "tsc -p server/tsconfig.json",
    "db:generate": "drizzle-kit generate:pg",
    "db:push": "drizzle-kit push:pg",
    "db:migrate": "tsx drizzle/migrate.ts",
    "db:seed": "tsx drizzle/seed.ts",
    "db:seed-properties": "tsx drizzle/seed-simple-properties.ts",
    "db:seed-social-media": "tsx drizzle/seed-social-media.ts",
    "db:create-social-media": "tsx drizzle/create-social-media-table.ts",
    "db:create-statistics": "tsx drizzle/create-statistics-table.ts",
    "db:create-settings": "tsx drizzle/create-settings-table.ts",
    "db:add-agent": "tsx drizzle/add-default-agent.ts",
    "db:update-agents": "tsx drizzle/update-agents-contact.ts",
    "db:migrate-cities": "tsx drizzle/migrate-cities.ts",
    "db:migrate-addresses": "tsx drizzle/migrate-addresses.ts",
    "check:users": "tsx drizzle/check-users.ts",
    "test:location-filter": "tsx drizzle/test-location-filter.ts",
    "fix:property-addresses": "tsx drizzle/fix-property-addresses.ts",
    "fix:remaining-addresses": "tsx drizzle/fix-remaining-addresses.ts",
    "final:address-fix": "tsx drizzle/final-address-fix.ts",
    "recreate:properties": "tsx drizzle/recreate-properties-clean.ts",
    "seed:clean-properties": "tsx drizzle/seed-clean-properties.ts",
    "test:addresses-api": "tsx drizzle/test-addresses-api.ts",
    "add:more-properties": "tsx drizzle/add-more-properties.ts",
    "create:features-table": "tsx drizzle/create-features-table.ts",
    "fix:features-table": "tsx drizzle/fix-features-table.ts",
    "recreate:features-table": "tsx drizzle/recreate-features-table.ts",
    "seed:features": "tsx drizzle/seed-features.ts",
    "test:features-system": "tsx drizzle/test-features-system.ts",
    "delete:all-properties": "tsx drizzle/delete-all-properties.ts",
    "seed:professional-properties": "tsx drizzle/seed-professional-properties.ts",
    "seed:enhanced-agents": "tsx drizzle/enhanced-agents-seed.ts",
    "update:existing-agents": "tsx drizzle/update-existing-agents.ts",
    "safe:agents-update": "tsx drizzle/safe-agents-update.ts",
    "add:basic-agents": "tsx drizzle/add-basic-agents.ts",
    "start": "node dist/server/index.js",
    "migrate-cities": "npm run db:migrate-cities",
    "migrate-addresses": "npm run db:migrate-addresses"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "drizzle-orm": "^0.29.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.7",
    "openai": "^4.20.1",
    "pg": "^8.11.3",
    "postgres": "^3.4.7",
    "socket.io": "^4.7.4",
    "twilio": "^4.19.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.4",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.10.9",
    "concurrently": "^8.2.2",
    "drizzle-kit": "^0.20.6",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  }
}
